@model Clothing

@{
    Layout = null;
}
<div class="modal-header remove-on-new-modal">
    <h5 class="modal-title" id="clothingDetailsModalTitle">@Html.DisplayFor(modelItem => Model.Name)</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body remove-on-new-modal">
    @await Html.PartialAsync("_ClothingMiniDetails", Model)
</div>